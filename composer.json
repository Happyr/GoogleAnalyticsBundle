{
    "name": "happyr/google-analytics-bundle",
    "type": "symfony-bundle",
    "description": "The Google Analytics Bundle lets you send data (like event tracking) to Google and receive data (like page statistics).",
    "keywords": ["Google", "Analytics", "Google Analytics"],
    "homepage": "http://developer.happyr.com/symfony2-bundles/google-analytics-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias@happyr.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/yaml": "2.*",
        "happyr/google-api-bundle": "2.*",
        "google/apiclient": "@beta",
        "unitedprototype/php-ga": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-0": {
            "HappyR\\Google\\AnalyticsBundle\\": ""
        }
    },
    "target-dir": "HappyR/Google/AnalyticsBundle"
}

